Host ct
	Hostname blah
	# VNC
	LocalForward 5900 localhost:5900
	RemoteForward 2200 localhost:22

Host dut-priv
	Hostname 192.168.0.2

Host dut-1 lnx-1
	Hostname my:test:net:work:7288:6bff:fe89:43dd
	# sslh
	LocalForward 2201 localhost:22
	# gdbserver
	LocalForward 12341 localhost:1234
	# renderdoc
	LocalForward 39920 localhost:39920

Host dut-2 lnx-2
	Hostname my:test:net:work:5e85:7eff:fe3a:b1d4
	LocalForward 2202 localhost:22
	LocalForward 12342 localhost:1234

Host dut-3 lnx-3
	Hostname my:test:net:work:5e85:7eff:fe3b:8382
	LocalForward 2203 localhost:22
	LocalForward 12343 localhost:1234

Host dut-borealis
	Hostname 100.115.92.26
	User chronos
	ProxyJump dut-3

Host dut-*
	User root
	CanonicalizeHostname yes
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	SetEnv TERM=xterm-256color

Host lnx-*
	User olv
	CanonicalizeHostname yes
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no

Host my:test:net:work:*
	ProxyCommand /usr/bin/ssh-helper %h %p

Host *
	ControlMaster auto
	ControlPath ~/.ssh/control-%r@%h:%p
	ControlPersist 10
	#ExitOnForwardFailure yes
