ControlMaster auto
ControlPath ~/.ssh/control-%r@%h:%p
ControlPersist 10
ExitOnForwardFailure yes

Host blah
	Hostname blah
	#RemoteForward 2230 192.168.86.30:22
	#RemoteForward 2237 192.168.86.37:22
	#LocalForward 5900 localhost:5900

Host dut-crosvm
	Hostname 192.168.0.2

Host dut-1
	ProxyCommand blah
	#LocalForward 2201 localhost:22
	User olv
	# renderdoc
	LocalForward 39920 localhost:39920

Host dut-2
	ProxyCommand blah
	LocalForward 2202 localhost:22

Host dut-*
	Hostname %h
	User root
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	SetEnv TERM=xterm

Host borealis-*
	Hostname 100.115.92.26
	User chronos
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	ProxyJump dut-%h
