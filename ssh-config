Host ct
	Hostname blah
	# VNC
	LocalForward 5900 localhost:5900
	# reverse ssh
	RemoteForward 2200 localhost:22
	# reverse adb over usb
	RemoteForward 5037 localhost:5037
	# reverse fastboot
	RemoteForward 5554 100.107.238.184:5554
	# reverse adb
	RemoteForward 5555 100.107.238.184:5555

Host x1
	Hostname localhost
	Port 2200

Host home
	Hostname olv.rip
	Port 51820

Host dut-priv lnx-priv
	Hostname 192.168.0.2

Host dut-1 lnx-1
	# UNITEK USB-A Black
	Hostname my:test:net:work:2e0:4cff:fe68:166
	# sslh
	LocalForward 2201 localhost:22
	# renderdoc
	LocalForward 39920 localhost:39920

Host dut-2 lnx-2
	# HP USB-C Hub
	Hostname my:test:net:work:3a7c:76ff:fe04:5f77
	LocalForward 2202 localhost:22

Host lnx-opi
	Hostname my:test:net:work:f487:cff:fe32:d5d5

Host dut-*
	User root
	CanonicalizeHostname yes
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	SetEnv TERM=xterm-256color

Host lnx-*
	User olv
	CanonicalizeHostname yes
	IdentityFile ~/.ssh/cros-testing_rsa
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no

Host my:test:net:work:*
	ProxyCommand /usr/bin/ssh-helper %h %p

Host *
	ControlMaster auto
	ControlPath ~/.ssh/control-%r@%h:%p
	ControlPersist 10
