Host ct
	Hostname blah
	# VNC
	LocalForward 5900 localhost:5900
	# reverse ssh
	RemoteForward 2200 localhost:22

Host x1
	Hostname localhost
	Port 2200
	# gdbserver
	LocalForward 1234 localhost:1234

Host dut-1 lnx-1
	# UNITEK USB-A
	Hostname my:test:net:work:2e0:4cff:fef0:f7
	# sslh
	LocalForward 2201 localhost:22
	# gdbserver
	LocalForward 12341 localhost:1234
	# renderdoc
	LocalForward 39920 localhost:39920

Host dut-2 lnx-2
	# Monoprice USB-C
	Hostname my:test:net:work:2e0:4cff:fe68:4398
	LocalForward 2202 localhost:22
	LocalForward 12342 localhost:1234

Host dut-3 lnx-3
	# Monoprice USB-A
